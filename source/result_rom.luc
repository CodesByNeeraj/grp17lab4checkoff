module result_rom (
    input address[4],  // ROM address
    output result[32]
) {
    
    const DATA = {
        32b00000000000000000000000000000011,
        32b00000000000000000000000000001011,
        32b11111111111111111111111111111110,
        32b00000000000000000000000000000111,
        32b11111111111111111111111111111001,
        32b00000000000000000000000000000000,
        32b00000000000000000000000000001100,
        32b11111111111111111111111111111100,
        32b00000000000000000000000000000100,
        32b00000000000000000000000000000000,
        32b00000000000000000000000000001111,
        32b00000000000000000000000000001111,
        32b00000000000000000000000000000000,
        32b00000000000000000000000000001111,
        32b00000000000000000000000000001111,
        32b00000000000000000000000000000001
    }
    
    always {
        result = DATA[address]
    }
}
